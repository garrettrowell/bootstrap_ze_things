{
  "puppet_task_version": 1,
  "supports_noop": false,
  "description": "Create a gitlab group",
  "parameters": {
    "host" : {
      "description": "Gitlab host to make https requests to.",
      "type": "String[1]"
    },
    "access_token": {
      "description": "The access token to use when making api requests.",
      "type": "String[1]",
      "sensitive": true
    },
    "repo_name": {
      "description": "The name of the new project.",
      "type": "String[1]"
    },
    "import_url": {
      "description": "URL to import repository from.",
      "type": "String[1]"
    },
    "namespace_id": {
      "description": "Namespace for the new project.",
      "type": "Integer"
    }

  }
}
